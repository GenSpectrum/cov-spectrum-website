{
  "name": "cov-spectrum-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^2.14.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "^4.12.4",
    "@mui/material": "^5.6.2",
    "@mui/x-data-grid": "^5.13.0",
    "@mui/x-date-pickers": "^5.0.0-alpha.6",
    "@sentry/react": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@types/bootstrap": "^5.0.5",
    "@types/chroma-js": "^2.1.3",
    "@types/d3": "^6.3.0",
    "@types/downloadjs": "^1.4.2",
    "@types/jest": "^28.1.6",
    "@types/json2csv": "^5.0.3",
    "@types/node": "^14.14.25",
    "@types/plotly.js": "^1.54.7",
    "@types/react": "^17.0.1",
    "@types/react-bootstrap": "^0.32.25",
    "@types/react-datepicker": "^4.1.7",
    "@types/react-dom": "^17.0.0",
    "@types/react-resize-detector": "^5.0.0",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.15",
    "@types/react-simple-maps": "^1.0.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/recharts": "^1.8.19",
    "@types/styled-components": "^5.1.7",
    "assert": "^2.0.0",
    "bootstrap": "^4.5.3",
    "chroma-js": "^2.1.1",
    "d3": "^6.5.0",
    "dayjs": "^1.11.4",
    "downloadjs": "^1.4.7",
    "json2csv": "^5.0.6",
    "plotly.js": "^1.58.4",
    "rc-slider": "^9.7.5",
    "react": "^17.0.2",
    "react-app-rewired": "^2.2.1",
    "react-async": "^10.0.1",
    "react-bootstrap": "^2.5.0",
    "react-component-export-image": "^1.0.6",
    "react-country-flag": "^3.0.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-multi-backend": "^6.0.2",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-numeric-input": "^2.2.3",
    "react-plotly.js": "^2.5.1",
    "react-resize-detector": "^6.6.0",
    "react-reverse-portal": "^2.1.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "^5.0.1",
    "react-select": "^4.3.0",
    "react-simple-maps": "^2.3.0",
    "react-test-renderer": "^17.0.1",
    "react-tooltip": "^4.2.14",
    "recharts": "^2.1.13",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4",
    "zod": "^3.18.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write . && npx eslint . --fix",
    "check-format": "prettier --check .",
    "check-types": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "no-unused-vars": "off",
      "unused-imports/no-unused-imports": "error",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ],
      "testing-library/no-unnecessary-act": "off"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/reportWebVitals.ts"
    ],
    "coveragePathIgnorePatterns": [
      "./src/*/*.types.{ts,tsx}",
      "./src/index.tsx",
      "./src/serviceWorker.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 95,
        "lines": 95,
        "functions": 95
      }
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|recharts)"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "src/(.*)$": "<rootDir>/src/$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/react-bootstrap-typeahead": "^5.1.2",
    "@types/react-dnd-multi-backend": "^6.0.2",
    "@types/react-numeric-input": "^2.2.4",
    "@types/react-plotly.js": "^2.2.4",
    "@types/react-router-hash-link": "^2.4.2",
    "all-contributors-cli": "^6.20.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.21.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest-canvas-mock": "^2.3.1",
    "jest-puppeteer": "^6.0.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "puppeteer": "^16.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2",
    "ts-jest": "^28.0.7"
  }
}
