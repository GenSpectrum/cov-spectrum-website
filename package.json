{
  "name": "cov-spectrum-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chatscope/chat-ui-kit-react": "^1.10.1",
    "@chatscope/chat-ui-kit-styles": "^1.4.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.13.4",
    "@mui/x-data-grid": "^7.18.0",
    "@mui/x-date-pickers": "^5.0.17",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bootstrap": "^5.2.6",
    "@types/chroma-js": "^2.1.4",
    "@types/d3": "^7.4.0",
    "@types/downloadjs": "^1.4.3",
    "@types/json2csv": "^5.0.3",
    "@types/node": "^18.15.3",
    "@types/plotly.js": "^2.33.3",
    "@types/react": "^18.0.28",
    "@types/react-bootstrap": "^0.32.32",
    "@types/react-resize-detector": "^6.1.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "bootstrap": "^5.2.3",
    "chroma-js": "^2.4.2",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "dayjs": "^1.11.7",
    "downloadjs": "^1.4.7",
    "json2csv": "^5.0.7",
    "jszip": "^3.10.1",
    "plotly.js": "^2.35.0",
    "react": "^18.2.0",
    "react-async": "^10.0.1",
    "react-bootstrap": "^2.7.2",
    "react-colorful": "^5.6.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-multi-backend": "^8.0.0",
    "react-dnd-preview": "^8.0.0",
    "react-dnd-test-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-icons": "^4.7.1",
    "react-plotly.js": "^2.6.0",
    "react-resize-detector": "^8.0.4",
    "react-reverse-portal": "^2.1.1",
    "react-router-dom": "^6.8.2",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.0",
    "react-test-renderer": "^18.2.0",
    "recharts": "^2.4.3",
    "styled-components": "^5.3.10",
    "typescript": "^4.9.5",
    "zod": "^3.22.4"
  },
  "overrides": {
    "@emotion/use-insertion-effect-with-fallbacks": {
      "react": "^18.2.0"
    },
    "d3-color": "^3.1.0",
    "svgo": "^3.0.2",
    "color-space": "2.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env REACT_APP_IS_TESTING=true react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write . && npx eslint . --fix",
    "check-format": "prettier --check .",
    "check-types": "tsc --noEmit",
    "check-dependencies": "depcheck"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "no-unused-vars": "off",
      "unused-imports/no-unused-imports": "error",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ],
      "testing-library/no-unnecessary-act": "off"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/reportWebVitals.ts"
    ],
    "coveragePathIgnorePatterns": [
      "./src/*/*.types.{ts,tsx}",
      "./src/index.tsx",
      "./src/serviceWorker.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 95,
        "lines": 95,
        "functions": 95
      }
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|recharts)"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "(lib-vendor/.*)": "victory-vendor/$1",
      "src/(.*)$": "<rootDir>/src/$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-dnd-multi-backend": "^6.0.2",
    "@types/react-dom": "^18.0.11",
    "@types/react-plotly.js": "^2.6.0",
    "@types/react-router-hash-link": "^2.4.5",
    "all-contributors-cli": "^6.24.0",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "eslint": "^8.34.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest-canvas-mock": "^2.5.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "puppeteer": "^19.7.2",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5"
  }
}
