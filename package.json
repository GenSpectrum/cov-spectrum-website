    "@mui/x-date-pickers": "^5.0.17",
    "@sentry/react": "^7.36.0",
    "@sentry/tracing": "^7.36.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bootstrap": "^5.2.6",
    "@types/chroma-js": "^2.1.4",
    "@types/d3": "^7.4.0",
    "@types/downloadjs": "^1.4.3",
    "@types/jest": "^29.4.0",
    "@types/json2csv": "^5.0.3",
    "@types/node": "^18.11.18",
    "@types/plotly.js": "^2.12.13",
    "@types/react": "^18.0.27",
    "@types/react-bootstrap": "^0.32.32",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-resize-detector": "^6.1.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-simple-maps": "^3.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "assert": "^2.0.0",
    "bootstrap": "^5.2.3",
    "chroma-js": "^2.4.2",
    "d3": "^7.8.2",
    "dayjs": "^1.11.7",
    "downloadjs": "^1.4.7",
    "json2csv": "^5.0.7",
    "jszip": "^3.10.1",
    "plotly.js": "^2.18.1",
    "rdndmb-html5-to-touch": "^8.0.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-async": "^10.0.1",
    "react-bootstrap": "^2.7.0",
    "react-colorful": "^5.6.1",
    "react-component-export-image": "^1.0.6",
    "react-country-flag": "^3.0.2",
    "react-dnd": "^16.0.1",
    "react-dnd-multi-backend": "^8.0.0",
    "react-dnd-preview": "^8.0.0",
    "react-dnd-test-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-plotly.js": "^2.6.0",
    "react-resize-detector": "^8.0.3",
    "react-reverse-portal": "^2.1.1",
    "react-router": "^6.8.0",
    "react-router-dom": "^6.8.0",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.0",
    "react-simple-maps": "^3.0.0",
    "react-test-renderer": "^18.2.0",
    "react-tooltip": "^5.7.4",
    "recharts": "^2.3.2",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.5",
    "web-vitals": "^3.1.1",
    "zod": "^3.20.2"
  },
  "overrides": {
    "@emotion/use-insertion-effect-with-fallbacks": {
      "react": "^18.2.0"
    },
    "d3-color": "^3.1.0",
    "svgo": "^3.0.2",
    "color-space": "2.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env REACT_APP_IS_TESTING=true react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write . && npx eslint . --fix",
    "check-format": "prettier --check .",
    "check-types": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "no-unused-vars": "off",
      "unused-imports/no-unused-imports": "error",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ],
      "testing-library/no-unnecessary-act": "off"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/reportWebVitals.ts"
    ],
    "coveragePathIgnorePatterns": [
      "./src/*/*.types.{ts,tsx}",
      "./src/index.tsx",
      "./src/serviceWorker.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 95,
        "lines": 95,
        "functions": 95
      }
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|recharts)"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "(lib-vendor/.*)": "victory-vendor/$1",
      "src/(.*)$": "<rootDir>/src/$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/react-bootstrap-typeahead": "^5.1.8",
    "@types/react-dnd-multi-backend": "^6.0.2",
    "@types/react-dom": "^18.0.10",
    "@types/react-numeric-input": "^2.2.4",
    "@types/react-plotly.js": "^2.6.0",
    "@types/react-router-hash-link": "^2.4.5",
    "all-contributors-cli": "^6.24.0",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-puppeteer": "^7.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "puppeteer": "^19.6.1",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.5"
  }
}
